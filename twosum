#include<bits/stdc++.h>
using namespace std;

int main(){
    int target;
    cin >> target;

    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }

    unordered_map<int,int> numMap;
    for(int i=0;i<n;i++){
        int complement = target - arr[i];
        if(numMap.count(complement)){
            cout << numMap[complement] << " " << i << endl;
            return 0;
        }
        numMap[arr[i]] = i;
    }

    cout << "No pair found" << endl;
    return 0;
}
