class Solution {
public:
    bool isAnagram(string s, string t) {
        if(s.size()!=t.size()){
            return false;
        }
        unordered_map<char,int> sc;
        unordered_map<char,int> tc;
        for(int i=0;i<s.size();i++){
            sc[s[i]]++;
            tc[t[i]]++;
        }
        return sc==tc;        
    }
};
