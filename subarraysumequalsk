class Solution {
public:
    int subarraySum(vector<int>& nums, int k) {
        unordered_map<int,int> mpp;
        mpp[0]=1;
        int c=0;
        int s=0;
        for(int i=0;i<nums.size();i++){
            s+=nums[i];
            int ans= s-k;
            if(mpp.count(ans)){
                c+=mpp[ans];
            }
            mpp[s]++;
        }
        return c;        
    }
};
