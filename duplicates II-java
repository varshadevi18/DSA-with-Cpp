class Solution {
    public boolean containsNearbyDuplicate(int[] nums, int k) {
        Map<Integer,Integer> mpp= new HashMap<>();
        for(int i=0; i<nums.length;i++){
            if(mpp.containsKey(nums[i])) return true;
            mpp.put(nums[i],i);   
            if(mpp.size() > k){
            mpp.remove(nums[i-k]);
        }
        }
        return false;
    }
}
