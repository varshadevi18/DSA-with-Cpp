class Solution {
public:
    int minSubArrayLen(int target, vector<int>& nums) {
        int l=0;
        int len=INT_MAX;
        int s=0;
        for(int r=0;r<nums.size();r++){
            s+=nums[r];
            while(s>=target){
                len=min(len,r-l+1);
                s-=nums[l];
                l++;
            }
        }
        return (len==INT_MAX)? 0: len;
        
    }
};
