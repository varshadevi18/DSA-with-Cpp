#include <bits/stdc++.h>
using namespace std;
int missing(int arr[],int ns, int N){
	int n=N-1;
	int XOR1=0;
	int XOR2=0;
	for(int i=0;i<n;i++){
	    XOR2=XOR2^arr[i];
	    XOR1=XOR1^(i+1);
	}
	XOR1=XOR1^N;
	return XOR1^XOR2;
}
int main() {
	// your code goes here
	int N;
	cin>>N;
	int ns;
	cin>>ns;
	int arr[ns];
	for(int i=0;i<ns;i++){
	    cin>>arr[i];
	}
	cout<<missing(arr,ns,N);

	
	
	

}
