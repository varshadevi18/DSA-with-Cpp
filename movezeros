#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }


    int k=-1;
    for(int i=0;i<n;i++){
        if(arr[i]==0){
            k=i;
            break;
        }
    }
    for(int j=k+1;j<n;j++){
        if(arr[j]!=0){
            swap(arr[k],arr[j]);
            k++;
        }
    }
    for(int m=0;m<n;m++){
        cout<<arr[m];
    }
}
