class Solution {
    public int[] twoSum(int[] nums, int target) {
        Map<Integer,Integer> mpp= new HashMap<>();
        for(int i=0;i<nums.length;i++){
            int c= target-nums[i];
            if(mpp.containsKey(c)){
                return new int[] {i, mpp.get(c)};
            }
            mpp.put(nums[i],i);
        }
        return new int[]{};
    }
}
