int pivotIndex(vector<int>& nums) {
        int total_sum=0;
        int ls=0;
        for(int i=0;i<nums.size();i++){
            total_sum+=nums[i];
            break;
        }
        for(int i=0;i<nums.size();i++){
            int rs=total_sum-ls-nums[i];
            if(ls==rs){
                return i;
            }
            ls+=nums[i];
        }
        return -1;
    }
};
