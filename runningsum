#include<bits/stdc++.h>
using namespace std;
int main(){
    int n;
    cin >> n;

    vector<int> arr(n);
    for(int i=0;i<n;i++){
        cin >> arr[i];
    }

    vector<int> r;
    int s=0;
    for(int i=0;i<n;i++){
        s+=arr[i];
        r.push_back(s);
    }
    for(int i=0;i<n;i++){
        cout<< r[i];
    }
}
